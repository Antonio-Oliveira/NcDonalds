
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - NcDonalds</title>
    
    <partial name="Account/_Stylus" />

</head>
<body>

    <header class="container-header">
        <a>
            <img src="~/images/logo1.png" class="img-header">
        </a>
        <div class="login">
            @RenderSection("navMessage", required: true)
        </div>
    </header>

    <main class="container-row ">
        @RenderBody()
    </main>

    <footer>
        <p>Copyright © NcDonald’s 2021</p>
    </footer>


    <partial name="Account/_Scripts" />

</body>
</html>
